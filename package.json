{
  "name": "claudectl",
  "version": "0.1.0",
  "description": "A powerful CLI tool for managing multiple parallel development sessions with Claude Code",
  "homepage": "https://github.com/HoganMcDonald/claudectl#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HoganMcDonald/claudectl.git"
  },
  "bugs": {
    "url": "https://github.com/HoganMcDonald/claudectl/issues"
  },
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "claudectl": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "lint": "biome lint src/",
    "format": "biome format --write src/",
    "check": "biome check --write src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:ci": "npm run typecheck && npm run lint && npm run test:run && npm run build",
    "setup:local": "npm run build && npm link",
    "setup:clean": "npm unlink -g claudectl",
    "prepare": "npm run build",
    "prepack": "npm run test:ci"
  },
  "keywords": [
    "cli",
    "claude",
    "claude-code",
    "git-worktree",
    "development-workflow",
    "parallel-development",
    "session-management",
    "typescript",
    "terminal",
    "tui"
  ],
  "author": "Hogan McDonald",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@types/node": "^24.3.0",
    "@types/tabtab": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.0.0",
    "happy-dom": "^18.0.1",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@types/react": "^19.1.10",
    "commander": "^11.0.0",
    "ink": "^6.2.0",
    "react": "^19.1.1",
    "tabtab": "^3.0.2",
    "zod": "^4.0.17"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
